      <section class="col-sm-10">
        <div id="main-top"></div>
        <div id="main-mid">
          <div class='row'>
            <table class='table-hover col-sm-12'>
              <tr>
                <th class='col-sm-2'>ISIN code</th>
                <th class='col-sm-5'>Security name</th>
                <th class='col-sm-1'>Currency</th>
                <th class='col-sm-2'>NAV price</th>
                <th class='col-sm-2'>Valuation date</th>
              </tr>
              <% @managers.each do |q| %>
              <tr class='manager'>
                <td colspan='5'>
                  <h4>
                    <a href='javascript:changeActiveManager("<%= q.name %>")'>
                      <div> 
                        <%= q.name %>                  
                      </div>
                    </a>
                  </h4>
                </td>
              </tr>
            </table>
            <% end %>
          </div>
        </div>
        <div id="main-bot"></div>
      </section>